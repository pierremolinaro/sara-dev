#---------------------------------------------------------------------------*#                                                                           *#   P R O G R A M                                                           *#                                                                           *#---------------------------------------------------------------------------*program sara_main "version 1.4.0" :  import grammar sara_grammar in "sara_grammar.ggs" ;  error 100 ;  warning 100 ;

  when . "sara" message "an '.sara' source file" ??@lstring inSourceFile {
    grammar sara_grammar in inSourceFile;
  }
end program ;#--------------------------------------------------------------------------------*